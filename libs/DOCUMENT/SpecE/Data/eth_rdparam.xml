                            <dd><table border="0" class="dd">
                                <tr> <td>0   </td><td>:</td><td>Connection one</td> </tr>
                                <tr> <td>1   </td><td>:</td><td>Connection two</td> </tr>
                                <tr> <td>2   </td><td>:</td><td>Connection three</td> </tr>
                            </table></dd>
