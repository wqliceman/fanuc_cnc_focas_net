    <example>
<vc>
The following program deletes all programs and displays the result.

#include "fwlib32.h"

void example( void )
{
        short ret ;
        ret = cnc_delall( h ) ;
        switch ( ret ) {
                case EW_OK:
                        printf( "ALL PROGRAMS has been deleted.\n", num ) ;
                        break;
                case EW_PROT:
                        printf( "PROTECTED.\n" ) ;
                        break;
                case EW_BUSY:
                        printf( "REJECTED.\n" ) ;
                        break;
        }
}
</vc>
