    <example>
<vc>
The method of judging "vacant" data.

#include "fwlib32.h"

short example( void )
{
    short       ret;
    unsigned long   s_no;
    unsigned long   num;
    unsigned short  type = 0;
    double      mcval;
    long        *rval;
    long        lval;
    long        hval;

    num = 1;
    ret = cnc_rdpmacror2( Hndl, s_no, &#38;num, type, &#38;mcval );
    rval = (long*)&#38;mcval;
    lval = (long)(*rval++);
    hval = (long)(*rval);
    if (( lval == (long)0xffffffff ) &#38;&#38; ( hval == (long)0xffffffff )) { 
        ; // "vacant"
    }

}
</vc>
